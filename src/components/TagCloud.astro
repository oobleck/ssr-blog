---
import { getTags } from '@utils/get-tags';
import Tags from './Tags.astro';
import {getTags} from '@utils/get-tags';

interface Props {
  type?: 'works' | 'blog';
}

const { type = 'blog' } = Astro.props;
const tags = await getTags(type);
---

<Tags tags={Object.keys(tags) ?? []} {type} layout="inline" />
